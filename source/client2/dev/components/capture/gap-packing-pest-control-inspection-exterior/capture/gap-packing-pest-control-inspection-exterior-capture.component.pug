extends ../../capture.common.pug

block reportViewer
  gap-packing-pest-control-inspection-exterior-report-viewer

block logViewer
  gap-packing-pest-control-inspection-exterior-log-list([suffix]='suffix', *ngIf='newLog !== true', (startLog)='onOpenLog($event)')
  gap-packing-pest-control-inspection-exterior-log(*ngIf='newLog == true', [log]='logData', (closeLog)='onCloseLog($event)')